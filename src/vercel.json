{
    "version": 2, 
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node-serve"
        },
        {
            "src": "./index.js.map",
            "use": "@vercel/nft"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}